{
  "name": "dd-annotation-uploader",
  "version": "1.0.0",
  "description": "DroneDeploy Annotation Uploader - Upload annotations via GraphQL API",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.6.2",
    "csv-parser": "^3.0.0",
    "xml2js": "^0.6.2",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "keywords": ["dronedeploy", "annotations", "graphql", "uploader"],
  "author": "",
  "license": "MIT"
}
